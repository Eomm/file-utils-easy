{
  "name": "file-utils-easy",
  "version": "1.1.0",
  "description": "The simplest file utils",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | node ./node_modules/coveralls/bin/coveralls.js",
    "docs": "jsdoc2md --template README.hbs --files index.js > README.md",
    "update-modules": "npm update && npm prune --production",
    "coverage": "npm test && npm run coveralls"
  },
  "keywords": [
    "file",
    "fs",
    "utils",
    "tool"
  ],
  "engines": {
    "node": ">=7.0.0"
  },
  "author": "Manuel Spigolon <behemoth89@gmail.com> (https://github.com/Eomm)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Eomm/file-utils-easy"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.27.2",
    "jest": "^22.4.4",
    "jsdoc-to-markdown": "^4.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
